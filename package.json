{
  "name": "wave-app",
  "version": "0.1.4",
  "description": "App for subscribing to practices",
  "main": "server/index.js",
  "scripts": {
    "start:app": "node server.js",
    "start:lambda": "netlify-lambda serve src/lambda",
    "clean": "run-p clean:*",
    "clean:app": "rm -rf dist",
    "build": "run-p build:**",
    "build:app": "npm run clean:app && parcel build src/index.html",
    "build:lambda": "netlify-lambda build src/lambda",
    "size": "npm run build:app && size-limit",
    "test": "npm run size"
  },
  "size-limit": [
    {
      "webpack": false,
      "path": "dist/*.js",
      "limit": "5s"
    }
  ],
  "cache": {
    "strategy": "inject",
    "swSrc": "./src/sw.js",
    "swDest": "sw.js",
    "inDev": true
  },
  "keywords": [
    "DSZ WAVE"
  ],
  "author": "Jorn Hoofwijk",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.1",
    "idb-keyval": "^3.2.0",
    "netlify-lambda": "^1.4.7",
    "npm-run-all": "^4.1.5",
    "pages": "0.0.16",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-sw-cache": "^0.2.7",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "size-limit": "^1.3.1",
    "workbox-cli": "^4.3.1"
  }
}
